<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.abgl.MyWaiterPad">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.abgl.MyWaiterPad.activity.TableListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.abgl.MyWaiterPad.activity.ProductsAvailableActivity">
        </activity>
        <activity android:name="com.abgl.MyWaiterPad.activity.AddProductSheetDetailActivity">
        </activity>
        <activity
            android:name="com.abgl.MyWaiterPad.activity.ProductListActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name="com.abgl.MyWaiterPad.activity.TableBillActivity">
        </activity>
        <activity android:name="com.abgl.MyWaiterPad.activity.ProductSheetActivity">
        </activity>
    </application>

</manifest>